{
  "name": "elm-mode",
  "version": "0.0.0",
  "private": true,
  "devDependencies": {
    "prettier-plugin-elm": "0.11.0"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-elm"
    ],
    "overrides": [
      {
        "files": "*.elm",
        "options": {
          "parser": "elm"
        }
      }
    ]
  }
}
